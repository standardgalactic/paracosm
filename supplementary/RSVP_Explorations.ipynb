    "# Parameters\n",
    "nx, ny = 50, 50  # Grid size\n",
    "x = np.linspace(-2*np.pi, 2*np.pi, nx)\n",
    "y = np.linspace(-2*np.pi, 2*np.pi, ny)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "\n",
    "# Define scalar potential Œ¶ (multi-well landscape for preferences)\n",
    "Phi = -np.cos(X) - np.cos(Y) + 0.1 * (X**2 + Y**2)  # Example: ethical potential with minima\n",
    "\n",
    "# Compute gradients (vector field v = -‚àáPhi)\n",
    "dPhi_dx, dPhi_dy = np.gradient(Phi, x, y)\n",
    "Vx = -dPhi_dx\n",
    "Vy = -dPhi_dy\n",
    "\n",
    "# Define gradient flow ODE\n",
    "def gradient_flow(state, t):\n",
    "    px, py = state\n",
    "    vx = np.interp(px, x, np.interp(py, y, Vx.T))\n",
    "    vy = np.interp(px, x, np.interp(py, y, Vy.T))\n",
    "    return [vx, vy]\n",
    "\n",
    "# Simulate trajectories from starting points\n",
    "start_points = [[0, 5], [5, 0], [-5, 0], [0, -5]]\n",
    "t = np.linspace(0, 10, 1000)\n",
