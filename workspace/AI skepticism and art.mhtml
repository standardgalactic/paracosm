Displays all PDFs, images, animations, and parchment texture.
"""</span>

</span><span><span class="hljs-keyword">from</span></span><span> pathlib </span><span><span class="hljs-keyword">import</span></span><span> Path

</span><span><span class="hljs-comment"># Collect files</span></span><span>
pdfs = </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.pdf"</span></span><span>))
illustrations = </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"illustration_*.png"</span></span><span>))
animations = </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.gif"</span></span><span>)) + </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.mp4"</span></span><span>))
parchment = Path(</span><span><span class="hljs-string">"parchment.jpg"</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> Path(</span><span><span class="hljs-string">"parchment.jpg"</span></span><span>).exists() </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>

</span><span><span class="hljs-comment"># Begin HTML</span></span><span>
