</span><span><span class="hljs-comment"># RSVP Build System</span></span><span>
</span><span><span class="hljs-comment"># Generates parchment first, then PDFs, then optional website</span></span><span>
</span><span><span class="hljs-comment"># -------------------------------------------------------------</span></span><span>

PARCHMENT = parchment.jpg
LATEX = island_of_equations intro_rsvp appendix_rsvp applied_rsvp
HTML = index.html

</span><span><span class="hljs-comment"># Default target</span></span><span>
</span><span><span class="hljs-section">all: <span class="hljs-variable">$(PARCHMENT)</span></span></span><span> pdfs site
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Full build complete."</span></span><span>

</span><span><span class="hljs-comment"># 1. Parchment generation -------------------------------------</span></span><span>
</span><span><span class="hljs-variable">$(PARCHMENT)</span></span><span>: make_parchment.py
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Generating parchment texture..."</span></span><span>
	python3 make_parchment.py

</span><span><span class="hljs-comment"># 2. PDF generation (depends on parchment) --------------------</span></span><span>
</span><span><span class="hljs-section">pdfs: $(LATEX:%=%.pdf)</span></span><span>

</span><span><span class="hljs-section">%.pdf: %.tex <span class="hljs-variable">$(PARCHMENT)</span></span></span><span>
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Compiling <span class="hljs-variable">$&lt;</span></span></span><span>..."
	pdflatex -interaction=nonstopmode </span><span><span class="hljs-variable">$&lt;</span></span><span>
	pdflatex -interaction=nonstopmode </span><span><span class="hljs-variable">$&lt;</span></span><span>
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Built <span class="hljs-variable">$@</span></span></span><span>"

</span><span><span class="hljs-comment"># 3. Website generation (depends on PDFs and parchment) -------</span></span><span>
</span><span><span class="hljs-section">site: <span class="hljs-variable">$(HTML)</span></span></span><span>
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Generating website..."</span></span><span>
	python3 build_site.py
	@echo </span><span><span class="hljs-string">"&gt;&gt;&gt; Website built successfully."</span></span><span>

</span><span><span class="hljs-comment"># 4. Utility targets ------------------------------------------</span></span><span>
</span><span><span class="hljs-section">clean:</span></span><span>
	rm -f *.aux *.log *.out *.toc *.fls *.fdb_latexmk
	@echo </span><span><span class="hljs-string">"Cleaned LaTeX temporary files."</span></span><span>

</span><span><span class="hljs-section">cleanall: clean</span></span><span>
	rm -f *.pdf *.html *.png *.gif *.mp4 </span><span><span class="hljs-variable">$(PARCHMENT)</span></span><span>
	@echo </span><span><span class="hljs-string">"Cleaned all generated files."</span></span><span>

</span><span><span class="hljs-meta"><span class="hljs-keyword">.PHONY</span></span></span><span>: all pdfs site clean cleanall
</span></span></code></div></div></pre>
