"""</span>

</span><span><span class="hljs-keyword">import</span></span><span> yaml
</span><span><span class="hljs-keyword">from</span></span><span> pathlib </span><span><span class="hljs-keyword">import</span></span><span> Path

manifest_path = Path(</span><span><span class="hljs-string">"build_manifest.yaml"</span></span><span>)
</span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> manifest_path.exists():
