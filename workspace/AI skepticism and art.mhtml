build_site.py
Creates a minimal HTML index linking to all PDFs and media in the current folder.
"""</span>

</span><span><span class="hljs-keyword">from</span></span><span> pathlib </span><span><span class="hljs-keyword">import</span></span><span> Path

pdfs = </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.pdf"</span></span><span>))
images = </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.gif"</span></span><span>)) + </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">"."</span></span><span>).glob(</span><span><span class="hljs-string">"*.mp4"</span></span><span>))

html = [
    </span><span><span class="hljs-string">"&lt;!DOCTYPE html&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;html&gt;&lt;head&gt;&lt;meta charset='utf-8'&gt;&lt;title&gt;Island of Equations&lt;/title&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;style&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"body{font-family:'Palatino Linotype',serif;background:#f2e8ce;"</span></span><span>,
    </span><span><span class="hljs-string">"color:#3f2a15;max-width:70ch;margin:3em auto;line-height:1.4}"</span></span><span>,
    </span><span><span class="hljs-string">"a{color:#3f2a15;text-decoration:none;border-bottom:1px dotted #3f2a15}"</span></span><span>,
    </span><span><span class="hljs-string">"a:hover{background:#efe5d0}"</span></span><span>,
    </span><span><span class="hljs-string">"img{max-width:100%;margin-top:1em;border:1px solid #3f2a15}"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;/style&gt;&lt;/head&gt;&lt;body&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;h1&gt;Island of Equations&lt;/h1&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;p&gt;Generated under the RSVP build system.&lt;/p&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;h2&gt;Documents&lt;/h2&gt;&lt;ul&gt;"</span></span><span>,
]
</span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> pdfs:
    html.append(</span><span><span class="hljs-string">f"&lt;li&gt;&lt;a href='<span class="hljs-subst">{f.name}</span></span></span><span>'&gt;</span><span><span class="hljs-subst">{f.stem}</span></span><span>&lt;/a&gt;&lt;/li&gt;")
html.append(</span><span><span class="hljs-string">"&lt;/ul&gt;&lt;h2&gt;Animations&lt;/h2&gt;"</span></span><span>)
</span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> images:
    html.append(</span><span><span class="hljs-string">f"&lt;p&gt;&lt;img src='<span class="hljs-subst">{f.name}</span></span></span><span>' alt='</span><span><span class="hljs-subst">{f.stem}</span></span><span>'&gt;&lt;/p&gt;")
html.append(</span><span><span class="hljs-string">"&lt;/body&gt;&lt;/html&gt;"</span></span><span>)

Path(</span><span><span class="hljs-string">"index.html"</span></span><span>).write_text(</span><span><span class="hljs-string">"\n"</span></span><span>.join(html), encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>)
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"index.html created."</span></span><span>)
