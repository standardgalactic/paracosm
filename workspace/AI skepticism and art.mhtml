]

</span><span><span class="hljs-comment"># Parchment preview section</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> parchment:
    html += [
        </span><span><span class="hljs-string">"&lt;div class='section'&gt;"</span></span><span>,
        </span><span><span class="hljs-string">"&lt;h2&gt;Parchment Texture&lt;/h2&gt;"</span></span><span>,
        </span><span><span class="hljs-string">f"&lt;img src='<span class="hljs-subst">{parchment.name}</span></span></span><span>' alt='Parchment texture preview'&gt;",
        </span><span><span class="hljs-string">"&lt;/div&gt;"</span></span><span>,
    ]

</span><span><span class="hljs-comment"># PDF section</span></span><span>
html += [</span><span><span class="hljs-string">"&lt;div class='section'&gt;&lt;h2&gt;Documents&lt;/h2&gt;&lt;ul class='pdf-list'&gt;"</span></span><span>]
</span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> pdfs:
    html.append(</span><span><span class="hljs-string">f"&lt;li&gt;&lt;a href='<span class="hljs-subst">{f.name}</span></span></span><span>'&gt;</span><span><span class="hljs-subst">{f.stem}</span></span><span>&lt;/a&gt;&lt;/li&gt;")
html.append(</span><span><span class="hljs-string">"&lt;/ul&gt;&lt;/div&gt;"</span></span><span>)

</span><span><span class="hljs-comment"># Illustration section</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> illustrations:
    html += [</span><span><span class="hljs-string">"&lt;div class='section'&gt;&lt;h2&gt;Illustrations&lt;/h2&gt;"</span></span><span>]
    </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> illustrations:
        html.append(</span><span><span class="hljs-string">f"&lt;img src='<span class="hljs-subst">{f.name}</span></span></span><span>' alt='</span><span><span class="hljs-subst">{f.stem}</span></span><span> illustration'&gt;")
    html.append(</span><span><span class="hljs-string">"&lt;/div&gt;"</span></span><span>)

</span><span><span class="hljs-comment"># Animation section</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> animations:
    html += [</span><span><span class="hljs-string">"&lt;div class='section'&gt;&lt;h2&gt;Animations&lt;/h2&gt;"</span></span><span>]
    </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> animations:
        </span><span><span class="hljs-keyword">if</span></span><span> f.suffix == </span><span><span class="hljs-string">".gif"</span></span><span>:
            html.append(</span><span><span class="hljs-string">f"&lt;img src='<span class="hljs-subst">{f.name}</span></span></span><span>' alt='</span><span><span class="hljs-subst">{f.stem}</span></span><span> animation'&gt;")
        </span><span><span class="hljs-keyword">elif</span></span><span> f.suffix == </span><span><span class="hljs-string">".mp4"</span></span><span>:
            html.append(</span><span><span class="hljs-string">f"&lt;video controls loop muted style='width:100%;margin-top:1em;'&gt;"</span></span><span>
                        </span><span><span class="hljs-string">f"&lt;source src='<span class="hljs-subst">{f.name}</span></span></span><span>' type='video/mp4'&gt;"
                        </span><span><span class="hljs-string">f"Your browser does not support MP4 video.&lt;/video&gt;"</span></span><span>)
    html.append(</span><span><span class="hljs-string">"&lt;/div&gt;"</span></span><span>)

</span><span><span class="hljs-comment"># Footer</span></span><span>
html += [
    </span><span><span class="hljs-string">"&lt;div class='section' style='text-align:center;font-size:0.9em;color:#5a4125;'&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;p&gt;All content  2025 Flyxion Research Group.&lt;br&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"Built automatically by the RSVP Makefile pipeline.&lt;/p&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;/div&gt;"</span></span><span>,
    </span><span><span class="hljs-string">"&lt;/body&gt;&lt;/html&gt;"</span></span><span>
]

</span><span><span class="hljs-comment"># Write to file</span></span><span>
Path(</span><span><span class="hljs-string">"index.html"</span></span><span>).write_text(</span><span><span class="hljs-string">"\n"</span></span><span>.join(html), encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>)
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"index.html created with parchment, PDFs, illustrations, and animations."</span></span><span>)
