        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Saved caldera_convection.gif"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> save_mp4:
        ani.save(</span><span><span class="hljs-string">"caldera_convection.mp4"</span></span><span>, writer=FFMpegWriter(fps=</span><span><span class="hljs-number">20</span></span><span>))
