</span><span><span class="hljs-built_in">set</span></span><span> -euo pipefail

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"&gt;&gt;&gt; Checking GitHub authentication..."</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> ! gh auth status &gt;/dev/null 2&gt;&amp;1; </span><span><span class="hljs-keyword">then</span></span><span>
  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">" You are not logged in. Run: gh auth login"</span></span><span>
  </span><span><span class="hljs-built_in">exit</span></span><span> 1
</span><span><span class="hljs-keyword">else</span></span><span>
  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">" Authenticated with GitHub."</span></span><span>
</span><span><span class="hljs-keyword">fi</span></span><span>

REPO=$(gh repo view --json nameWithOwner -q </span><span><span class="hljs-string">'.nameWithOwner'</span></span><span>)

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"&gt;&gt;&gt; Ensuring GitHub Pages is enabled..."</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> ! gh api repos/</span><span><span class="hljs-variable">$REPO</span></span><span>/pages &gt;/dev/null 2&gt;&amp;1; </span><span><span class="hljs-keyword">then</span></span><span>
  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">" Pages not yet enabled. Enabling..."</span></span><span>
  gh api -X POST repos/</span><span><span class="hljs-variable">$REPO</span></span><span>/pages \
    --silent \
    --field build_type=legacy \
    --field </span><span><span class="hljs-built_in">source</span></span><span>=</span><span><span class="hljs-string">'{"branch":"main","path":"/"}'</span></span><span> || </span><span><span class="hljs-literal">true</span></span><span>
</span><span><span class="hljs-keyword">else</span></span><span>
  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">" Pages already enabled."</span></span><span>
</span><span><span class="hljs-keyword">fi</span></span><span>

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"&gt;&gt;&gt; Building site..."</span></span><span>
make clean
make all

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"&gt;&gt;&gt; Committing and pushing..."</span></span><span>
git add -A
</span><span><span class="hljs-keyword">if</span></span><span> git diff --cached --quiet; </span><span><span class="hljs-keyword">then</span></span><span>
  </span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"No changes to commit."</span></span><span>
</span><span><span class="hljs-keyword">else</span></span><span>
  git commit -m </span><span><span class="hljs-string">"Automated build <span class="hljs-subst">$(date '+%Y-%m-%d %H:%M:%S')</span></span></span><span>"
</span><span><span class="hljs-keyword">fi</span></span><span>
git push origin main

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"&gt;&gt;&gt; Deploying to GitHub Pages..."</span></span><span>
gh pages deploy --branch=main --dist=. --message </span><span><span class="hljs-string">"Deploy <span class="hljs-subst">$(date '+%Y-%m-%d %H:%M:%S')</span></span></span><span>"

</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">" Deployment complete!"</span></span><span>
</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"Visit: https://<span class="hljs-variable">${REPO%/*}</span></span></span><span>.github.io/</span><span><span class="hljs-variable">${REPO#*/}</span></span><span>/"
</span></span></code></div></div></pre>
