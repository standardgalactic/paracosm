by_cat = defaultdict(</span><span><span class="hljs-built_in">list</span></span><span>)
</span><span><span class="hljs-keyword">for</span></span><span> p </span><span><span class="hljs-keyword">in</span></span><span> projects:
    by_cat[p.get(</span><span><span class="hljs-string">"category"</span></span><span>, </span><span><span class="hljs-string">"Misc"</span></span><span>)].append(p)

</span><span><span class="hljs-keyword">for</span></span><span> category, plist </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">sorted</span></span><span>(by_cat.items()):
    html.append(</span><span><span class="hljs-string">f"&lt;h2&gt;<span class="hljs-subst">{category}</span></span></span><span>&lt;/h2&gt;")
    </span><span><span class="hljs-keyword">for</span></span><span> p </span><span><span class="hljs-keyword">in</span></span><span> plist:
