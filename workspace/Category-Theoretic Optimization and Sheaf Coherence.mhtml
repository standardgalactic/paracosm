<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">verbatim</span><span style="color: rgb(230, 230, 230);">}</span></span>
