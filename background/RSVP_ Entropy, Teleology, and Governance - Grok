.monaco-text-button.monaco-text-button-with-short-label > .monaco-button-label-short { flex-grow: 1; overflow: hidden; width: 0px; }

.monaco-text-button.monaco-text-button-with-short-label > .monaco-button-label, .monaco-text-button.monaco-text-button-with-short-label > .monaco-button-label-short { align-items: center; display: flex; font-style: inherit; font-weight: 400; justify-content: center; padding: 4px 0px; }

.monaco-button-dropdown { cursor: pointer; display: flex; }

.monaco-button-dropdown.disabled { cursor: default; }

