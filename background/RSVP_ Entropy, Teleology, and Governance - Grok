[
\mathbf{Matter} \xrightarrow{\mathcal{F}_1} \mathbf{Blanket} \xrightarrow{\mathcal{F}_2} \mathbf{Preference} \xrightarrow{\mathcal{F}_3} \mathbf{Desire}
]
